      <Act name="Behavior">

        <ManeuverGroup maximumExecutionCount="1" name="SimpleScenario1Sequence_1">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="vehicle_0"/>
          </Actors>
          <ScenarioReference scenarioFileName="simple_1.xosc">
            <ParameterReference key="AbsoluteTargetSpeed.value" value="20"/>
